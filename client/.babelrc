{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "useBuiltIns": "entry",
          "corejs": {
            "version": 3,
            "proposals": true
          },
          "targets": "Chrome >= 87, Safari >= 15, iOS >= 15, Firefox >= 77"
        }
      }
    ]
  ],
  "plugins": [["styled-components", { "ssr": true, "transpileTemplateLiterals": true }]]
}
